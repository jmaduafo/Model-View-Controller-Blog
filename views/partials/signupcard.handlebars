<section>
    <div class="signup-form">
        <div class="signup-contents">
            <div class="top-panel">
                <div class="log-image">
                    <img src="/images/Astronomical_symboL.png" alt="site logo">
                </div>
            </div>
            <div class="circle"></div>
            <div class="bottom-panel">
                <form class="log-form">
                    <div class="form-inserts">
                        <input type="email" id="email-signup" placeholder="Email">
                        <input type="text" id="username-signup" placeholder="Username">
                        <input type="password" id="password-signup" placeholder="Password">
                    </div>
                    <div class="form-button">
                        <button type="submit">Create Account</button>
                    </div>
                </form>
                <div class="user old-user">
                    <p>Already have an account?</p>
                    <a href="/login"><p>Switch to Log In!</p></a>
                </div>
            </div>             
        </div>
    </div>
</section>